{
    "default_locale": "en",
    "manifest_version": 2,
    "name": "__MSG_extName___",
    "description": "__MSG_extDescription___",
    "version": "0.2.5",
    "author": "slikts <dabas@untu.ms",
    "permissions": [
        "http://localhost:8111/"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "icons": {
        "128": "images/icon-128.png",
        "16": "images/icon-16.png",
        "48": "images/icon-48.png"
    },
    "options_page": "options.html",
    "homepage_url": "https://github.com/slikts/wtmap",
    "background": {
        "scripts": ["scripts/background.js"],
        "persistent": false
    },
    "optional_permissions": ["http://*/"],
    "content_scripts": [
        {
            "matches": ["http://localhost:8111/*"],
            "js": ["vendor/jquery-1.10.2.min.js", "scripts/redirect.js"]
        }
    ]
}
